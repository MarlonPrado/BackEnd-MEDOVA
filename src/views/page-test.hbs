  <style>
    .navbar {
      display: flex;
      justify-content: space-between;
      align-items: center;
      padding: 10px;
      background-color: #333;
      color: #fff;
    }

    .navbar .menu-icon {
      font-size: 24px;
      cursor: pointer;
    }

    .navbar .lock-icon {
      display: none;
      font-size: 18px;
      margin-right: 10px;
      color: red;
    }

    .navbar.unlocked .menu-icon {
      display: inline-block;
    }

    .navbar.locked .menu-icon {
      display: none;
    }

    .navbar.locked .lock-icon {
      display: inline-block;
    }

    .navbar .menu-items {
      display: none;
      padding: 10px;
      background-color: #f1f1f1;
      color: #333;
    }

    .navbar.unlocked .menu-items {
      display: block;
    }

    .navbar.locked .menu-items {
      display: none;
    }
  </style>

</head>

<body data-typography="poppins" data-theme-version="light" data-layout="vertical">


