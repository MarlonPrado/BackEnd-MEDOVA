        <body>

  <nav class="navbar locked">
    <i class="fas fa-lock lock-icon"></i>
    <i class="fas fa-bars menu-icon"></i>
    <div class="menu-items">
      <ul>
        <li><a href="#">Inicio</a></li>
        <li><a href="#">Acerca</a></li>
        <li><a href="#">Servicios</a></li>
        <li><a href="#">Contacto</a></li>
      </ul>
    </div>
  </nav>

  <script>
    // Verificar si se ha completado el nivel anterior
    var nivelAnteriorCompletado = false; // Cambia esto según tus necesidades
    
    // Obtener elementos
    var navbar = document.querySelector('.navbar');

    // Agregar o quitar clases según el estado del nivel anterior
    if (nivelAnteriorCompletado) {
      navbar.classList.remove('locked');
      navbar.classList.add('unlocked');
    } else {
      navbar.classList.remove('unlocked');
      navbar.classList.add('locked');
    }
  </script>

        </body>

